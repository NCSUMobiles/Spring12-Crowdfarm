    <style name="registerText" parent="pageText">
        <item name="android:textSize">25sp</item>
    </style>
    <style name="homeBtn" parent="btnText">
        <item name="android:textSize">35sp</item>
    </style>
    <style name="loginBtn" parent="btnText">
        <item name="android:textSize">25sp</item>
