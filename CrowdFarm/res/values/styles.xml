        <item name="android:textSize">50sp</item>
        <item name="android:textColor">#385A28</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="appBackground" parent="android:Theme.NoTitleBar">
        <item name="android:background">#C2B88F</item>
    </style>
    <style name="btnText">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#D6D6AB</item>
